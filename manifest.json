{
    "manifest_version": 2,
    "name": "SWAP",
    "description": "Swap Those Ugly Words! 
    Try this extension to swap the triggering words by some funnier ones!
    Compatible with Chrome only - for now -","version": "0.10",
    "browser_action": {
        "default_popup": "swap.html",
        "default_icon": {
            "16": "/Images/swap16.png",
            "32": "/Images/swap32.png",
            "128": "/Images/swap128.png"
        }
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            }
        }
    },
    "icons": {
        "16": "/Images/swap16.png",
        "32": "/Images/swap32.png",
        "128": "/Images/swap128.png"
    }
}